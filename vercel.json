{
    "version": 2,
    "name": "Job-App",
    "builds": [
        {
            "src": "Frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "Frontend/build"
            }
        }
        
    ],
    "routes": [
        
        {
            "src": "/(.*)",
            "dest": "/Frontend/build/index.html"
        }
    ]
}