{
    "version": 2,
    "name": "job-app",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "../Frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "../Frontend/build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "../Frontend/build/index.html"
        }
    ]
}